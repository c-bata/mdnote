    "gulp-sourcemaps": "^1.5.2",
    "gulp-uglify": "^1.2.0",
    "gulp-watchify": "^0.5.0",
    "jquery": "^2.1.4",
    "md2react": "^0.8.8",
    "mocha": "^2.2.5",
    "power-assert": "^0.11.0",
